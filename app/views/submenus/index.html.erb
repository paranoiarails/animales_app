<h1>Listing submenus</h1>

<table>
  <tr>
    <th>Menu</th>
    <th>Titulo</th>
    <th>Contenido</th>
    <th>Picture file name</th>
    <th>Picture content type</th>
    <th>Picture file size</th>
    <th>Picture updated at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @submenus.each do |submenu| %>
  <tr>
    <td><%= submenu.menu_id %></td>
    <td><%= submenu.titulo %></td>
    <td><%= submenu.contenido %></td>
    <td><%= submenu.Picture_file_name %></td>
    <td><%= submenu.Picture_content_type %></td>
    <td><%= submenu.Picture_file_size %></td>
    <td><%= submenu.Picture_updated_at %></td>
    <td><%= link_to 'Show', submenu %></td>
    <td><%= link_to 'Edit', edit_submenu_path(submenu) %></td>
    <td><%= link_to 'Destroy', submenu, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Submenu', new_submenu_path %>
